{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/greetings.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/greetings/(.*)",
      "dest": "/api/greetings.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
